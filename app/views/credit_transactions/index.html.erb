<p id="notice"><%= notice %></p>

<h1>Credit Transactions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @credit_transactions.each do |credit_transaction| %>
      <tr>
        <td><%= link_to 'Show', credit_transaction %></td>
        <td><%= link_to 'Edit', edit_credit_transaction_path(credit_transaction) %></td>
        <td><%= link_to 'Destroy', credit_transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Credit Transaction', new_credit_transaction_path %>
